{
  "manifest_version": 2,
  "icons": {"128": "icon.png"},
  "name": "One-click music search",
  "description": "This extension finds music for us",
  "version": "1.0",

  "permissions": [
    "https://www.google.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
